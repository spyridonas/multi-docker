    <table class="table">
        <tbody>
            <tr>
                <th>Email_id</th>
                <td>{{ email.emailId }}</td>
            </tr>
            <tr>
                <th>Email_literal</th>
                <td>{{ email.emailLiteral }}</td>
            </tr>
            <tr>
                <th>Email_provider</th>
                <td>{{ email.emailProvider }}</td>
            </tr>
            <tr>
                <th>Email_primary</th>
